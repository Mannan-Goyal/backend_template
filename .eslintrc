{
    "env": {
      "node": true,
      "es2022": true,
      "jest": true
    },
    "extends": ["xo", "prettier"],
    "overrides": [
      {
        "extends": ["xo-typescript", "prettier"],
        "files": ["*.ts", "*.tsx"]
      }
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["prettier", "@typescript-eslint", "import"],
    "rules": {
      "prettier/prettier": "error",
      "import/extensions": 0,
      "no-console": 0,
      "indent": ["error", 4]
    }
  }
  